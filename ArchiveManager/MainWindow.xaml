<Window x:Class="ArchiveManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArchiveManager"
        mc:Ignorable="d"
        Title="ArchiveManager" Height="450" Width="800">
    <Grid>
        <TabControl Margin="10,0,10,10">
            <TabItem Header="Animes">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Games">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Books">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Films">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
